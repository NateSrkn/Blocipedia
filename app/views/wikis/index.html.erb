<div class="content">

<h1>All Wikis</h1>

    <% if policy(@wikis).new? %>
        <h2><%= link_to 'New Wiki', new_wiki_path, class: 'link' %></h2>
    <% end %>

    <div id="wikis">
        <% @wikis.each do |wiki| %>
        <div class="wiki-choice">
            <h3><%= link_to wiki.title, wiki %></h3>
            <p>
                <%= wiki.user.email %>
            </p>

        </div>
        <% end %>
    </div>
</div>